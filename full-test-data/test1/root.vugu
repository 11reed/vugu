<span class="test-span" id="test_span_id">
    This is a test.
    <button id="run" @click="data.OnClickRun(event)">run</button>
    <div id="success" vg-if="data.Success">success</div>
</span>

<script type="application/x-go">
type Root struct {
    Success bool
}

func (c *Root) OnClickRun(event *vugu.DOMEvent) {
    c.Success = true
}
</script>
